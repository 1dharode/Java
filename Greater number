import java.util.*;
class FirstClass{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the value of a : ");
        int a=sc.nextInt();
        System.out.print("Enter the value of b : ");
        int b=sc.nextInt();
        if(a>b){
            System.out.print(a);
            System.out.print(" is greater than ");
            System.out.print(b);
        }
        else{
            System.out.print(b);
            System.out.print(" is greater than ");
            System.out.print(a);
        }
    }
}
